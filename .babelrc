{
    "overrides": [
        {
            "exclude": "**/inline-script.raw.js",
            "presets": [
                ["@moxy/babel-preset/lib", { "react": true }]
            ],
            "plugins": [
                ["babel-plugin-inline-import", { "extensions": [".raw.js", ".raw"] }]
            ]
        },
        {
            "test": "**/inline-script.raw.js",
            "presets": [
                ["@moxy/babel-preset/end-project", {
                    "targets": {
                        "browsers": ["IE 11"]
                    }
                }]
            ]
        }
    ]
}
